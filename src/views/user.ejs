<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" type="text/css" href="css/main.css" />
    <title>Comrades Cove</title>
    <script
      src="https://kit.fontawesome.com/f9371e1f01.js"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <div class="nav">
      <a href="/home"
        ><img id="logo" src="https://i.ibb.co/DgLFTpV/Comrades-Cove.png" alt=""
      /></a>
      <h1 id="title">Comrades Cove</h1>
      <div class="inline">
        <a href="/user"><%- avatar %></a>
      </div>
    </div>
    <div class="data">
      <h1><%= username %></h1>
      <h3><%= email %></h3>
      <h3><%= id %></h3>

      <ul>
        <% for(var i = 0; i < guilds.length; i++) { %>
        <li><%= guilds[i].name %></li>
        <% } %>
      </ul>
    </div>
  </body>
</html>
